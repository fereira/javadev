                        <citation>@incollection{Bruno2013a, abstract= {agINFRA {\`{e}} un progetto che ha lo scopo di sviluppare strumenti per supportare la comunit{\`{a}} delle scienze agrarie basati sull’uso di infrastrutture dati distribuite a livello europeo e intercontinentale, insistenti sulle reti nazio- nali ed internazionali della ricerca. Quest’articolo descrive l’architettura dello che {\`{e}} stato sviluppato per agINFRA basandosi sul Cata- nia e illustra le sue caratteristiche di ambiente integrato per l’accesso e la gestione dei dati, offerte mediante l’adozione dei paradigmi sia di Grid computing che di cloud computing. In particolare vengo- no descritti degli standard adottati, quali SAGA e SAML, e la loro importanza per la sostenibilit{\`{a}} dell’infrastruttura e dei suoi servizi. A titolo di esempio, saranno mostrate alcune delle applicazioni disponibili agli utenti attraverso lo Science Gateway}, author= {Bruno, Riccardo and Allegri, Giovanni and Andronico, Giuseppe and Barbera, Roberto and Bitelli, Federico and Budano, Antonio and Calanducci, Antonio and Costantini, Edoardo A.C. and Fargetta, Marco and Fornaia, Andrea and L'Abate, Giovanni and Monforte, Salvatore and Puliafito, Antonio and Ricceri, Rita and Ruggieri, Federico and Saitta, Davide and Villari, Massimo}, booktitle= {Workshop GARR CSD Selected papers CALCOLO E STORAGE DISTRIBUITO Roma, 29-30 novembre 2012}, file= {:F$\backslash$:/Documenti/Pubblicazioni/2013/GARR/Bruno2013-Lo-Science-Gateway-del-progetto-agINFRA.pdf:pdf}, isbn= {978-88-905077-4-8}, keywords= {Computer Science,Infrastructure,and Data Structures  Share}, pages= {33--39}, publisher= {Associazione Consortium GARR}, title= {{Lo Science Gateway del progetto agINFRA per l’accesso a una data Infrastructure per le Scienze Agrarie}}, url= {http://www.garr.it/a/garr/documenti-chisiamo/doc{\_}download/1858--selected-papers-workshop-csd-garr-2012-volume}, year= {2013}}

